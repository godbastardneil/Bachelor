CREATE PROCEDURE public.set_ball_to_ex(id_dt integer, idex integer, ans text, b integer)
    LANGUAGE plpgsql
    AS $$
BEGIN
	INSERT INTO список_ответов (idсписок_тестов, idзадание, балл, ответ_пользователя) 
	VALUES (id_dt, idex, b, ans);
END;
$$;


ALTER PROCEDURE public.set_ball_to_ex(id_dt integer, idex integer, ans text, b integer) OWNER TO ege;

--
-- TOC entry 245 (class 1255 OID 16571)
-- Name: set_ball_to_test(text, integer, integer); Type: PROCEDURE; Schema: public; Owner: ege
--

CREATE PROCEDURE public.set_ball_to_test(login text, id_do_t integer, idt integer)
    LANGUAGE plpgsql
    AS $$
declare
	is_prop text;
	ball int;
	max_ball int;
BEGIN
	ball :=
		(select SUM(балл)
		 from список_ответов
		 where idсписок_тестов = id_do_t);
	max_ball :=
		(select лучший_результат
		 from назначенные_тесты
		 where idтест = idt);
    
    is_prop :=
		(select string_agg(ответ_пользователя, '')
		 from список_ответов
		 where idсписок_тестов = id_do_t);
		 
		 
	UPDATE список_тестов
	SET балл = ball
	WHERE idсписок_тестов = id_do_t;
	
	UPDATE назначенные_тесты
	SET idстатус_теста = 2
	WHERE idтест = idt;
		 
	if (ball > max_ball) then
		UPDATE назначенные_тесты
		SET лучший_результат = ball
		WHERE idтест = idt;
    end if;
    
    if (is_prop = '') then
		UPDATE статистика_ученика
		SET кол_во_пропущенных_тестов = кол_во_пропущенных_тестов+1
		WHERE логин = login;
    end if;
END;
$$;


ALTER PROCEDURE public.set_ball_to_test(login text, id_do_t integer, idt integer) OWNER TO ege;

