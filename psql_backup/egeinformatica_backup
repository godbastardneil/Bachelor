CREATE PROCEDURE public.appointment_test(idt integer, login text)
    LANGUAGE plpgsql
    AS $$
BEGIN
	INSERT INTO назначенные_тесты (лог_получатель, idтест, idстатус_теста)
	VALUES (login, idt, 1);
END;
$$;


ALTER PROCEDURE public.appointment_test(idt integer, login text) OWNER TO ege;

--
-- TOC entry 223 (class 1255 OID 16560)
-- Name: check_user(text, text); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.check_user(login text, pas text) RETURNS TABLE("логин" text, "роль" text)
    LANGUAGE plpgsql
    AS $$
BEGIN
	return query
	select a.логин, b.название
		from пользователь a, роль b
		where a.логин = login
				and a.пароль = pas
				and a.idроль = b.idроль;
END;
$$;


ALTER FUNCTION public.check_user(login text, pas text) OWNER TO ege;

--
-- TOC entry 224 (class 1255 OID 16561)
-- Name: create_do_test(text, integer); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.create_do_test(login text, idt integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
DECLARE
		last_id integer;
BEGIN
	INSERT INTO список_тестов (лог_получатель, idтест) 
	VALUES (login, idt) RETURNING idсписок_тестов INTO last_id;

	UPDATE статистика_ученика
	SET кол_во_тестов = кол_во_тестов+1 
	WHERE логин = login;

	return last_id;
END;
$$;


ALTER FUNCTION public.create_do_test(login text, idt integer) OWNER TO ege;

--
-- TOC entry 225 (class 1255 OID 16562)
-- Name: create_test(text, integer, integer); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.create_test(login text, n integer, "id_тип" integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
DECLARE
		last_id integer;
BEGIN
	INSERT INTO тест (лог_создатель, idтип_теста, кол_во_заданий) 
	VALUES (login, id_тип, n) RETURNING idтест INTO last_id;

	return last_id;
END;
$$;


ALTER FUNCTION public.create_test(login text, n integer, "id_тип" integer) OWNER TO ege;

--
-- TOC entry 237 (class 1255 OID 16563)
-- Name: get_do_test(text); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.get_do_test(login text) RETURNS TABLE("idтест" integer, "статус" text, "задания" text)
    LANGUAGE plpgsql
    AS $$
declare
	teacher_log text;
BEGIN
	teacher_log := (select лог_преподаватель from класс where лог_ученик = login);
	
	return query
	select a.idтест, c.название, string_agg(b.название, '<br>') as задание
	from назначенные_тесты a, тип_задания b, статус_теста c
	where a.лог_получатель = login
			AND NOT (login = (select лог_создатель from тест d where d.idтест = a.idтест))
			AND a.idстатус_теста = c.idстатус_теста
			AND b.idтип_задания IN (select idтип_задания
									 from тип_задания
									 where idтип_задания IN (select idтип_задания
															 from вариант_задания
															 where idвариант_задания IN (select idвариант_задания
																						 from вопрос
																						 where idвопрос IN (select idвопрос
																											 from задание
																											 where idзадание IN (select idзадание
																																 from список_заданий e
																																 where e.idтест = a.idтест)))))
	 group by a.idтест, a.лучший_результат, c.название;
END;
$$;


ALTER FUNCTION public.get_do_test(login text) OWNER TO ege;

--
-- TOC entry 238 (class 1255 OID 16564)
-- Name: get_main_stat(text); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.get_main_stat(login text) RETURNS TABLE(_max double precision, _min double precision, _sred double precision)
    LANGUAGE plpgsql
    AS $$
DECLARE
		kol int;
		kol_ex int;
BEGIN
	kol :=
		(select кол_во_тестов
		 from статистика_ученика
		 where логин = login);

	return query
	select MAX(a.балл::double precision/b.макс_балл),
			MIN(a.балл::double precision/b.макс_балл),
			SUM(a.балл::double precision/b.макс_балл)/kol
	FROM список_тестов a, тест b
	WHERE a.лог_получатель = login AND a.idтест = b.idтест;
END;
$$;


ALTER FUNCTION public.get_main_stat(login text) OWNER TO ege;

--
-- TOC entry 239 (class 1255 OID 16565)
-- Name: get_right_ans(integer); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.get_right_ans(idt integer) RETURNS TABLE("idзадание" integer, "правильный_ответ" text)
    LANGUAGE plpgsql
    AS $$
BEGIN
	return query
	select a.idзадание, b.правильный_ответ
	from список_заданий a, задание b
	where a.idтест = idt
			AND a.idзадание = b.idзадание
	ORDER BY номер;
END;
$$;


ALTER FUNCTION public.get_right_ans(idt integer) OWNER TO ege;

--
-- TOC entry 240 (class 1255 OID 16566)
-- Name: get_stat_test(text); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.get_stat_test(login text) RETURNS TABLE("idтест" integer, "лог_создатель" text, "отношение" text, "баллы" text)
    LANGUAGE plpgsql
    AS $$
BEGIN
	return query
	select a.idтест,
			b.лог_создатель,
			concat(c.лучший_результат::text, '/', b.макс_балл::text) as отношение,
			string_agg(a.балл::text, ', ')
	from список_тестов a, тест b, назначенные_тесты c
	where a.лог_получатель = login
		AND a.idтест = b.idтест AND a.idтест = c.idтест
	group by a.idтест, b.лог_создатель, c.лучший_результат, b.макс_балл order by a.idтест;
END;
$$;


ALTER FUNCTION public.get_stat_test(login text) OWNER TO ege;

--
-- TOC entry 241 (class 1255 OID 16567)
-- Name: get_tasks(integer); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.get_tasks(idt integer) RETURNS TABLE("текст" text)
    LANGUAGE plpgsql
    AS $$
BEGIN
	return query
	select b.текст
	from список_заданий a, задание b
	where a.idтест = idt
			AND a.idзадание = b.idзадание
	ORDER BY номер;
END;
$$;


ALTER FUNCTION public.get_tasks(idt integer) OWNER TO ege;

--
-- TOC entry 242 (class 1255 OID 16568)
-- Name: include_ex_to_test(integer, integer, integer); Type: PROCEDURE; Schema: public; Owner: ege
--

CREATE PROCEDURE public.include_ex_to_test(idt integer, idex integer, num integer)
    LANGUAGE plpgsql
    AS $$
BEGIN
	INSERT INTO список_заданий (idзадание, idтест, номер)
	VALUES (idex, idt, num);
    
    UPDATE тест
	SET макс_балл = макс_балл+(select макс_балл from задание where idзадание = idex)
	WHERE idтест = idt;
END;
$$;


ALTER PROCEDURE public.include_ex_to_test(idt integer, idex integer, num integer) OWNER TO ege;

--
-- TOC entry 243 (class 1255 OID 16569)
-- Name: include_exersize(integer, text, text); Type: FUNCTION; Schema: public; Owner: ege
--

CREATE FUNCTION public.include_exersize(idv integer, _text text, ans text) RETURNS integer
    LANGUAGE plpgsql
    AS $$
DECLARE
		last_id integer;
BEGIN
	INSERT INTO задание (idвопрос, текст, правильный_ответ, макс_балл)
	VALUES (idv, _text, ans, (select макс_балл
							 from тип_задания
							 where idтип_задания = (select idтип_задания
													 from вариант_задания
													 where idвариант_задания = (select idвариант_задания
																				 from вопрос
																				 where idвопрос = 1))))
	RETURNING idзадание INTO last_id;

	return last_id;
END;
$$;


ALTER FUNCTION public.include_exersize(idv integer, _text text, ans text) OWNER TO ege;

--
